                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:elevation="2dp"
                    app:cardBackgroundColor="#fac1c5"
                    app:cardElevation="2dp">

                    <LinearLayout
