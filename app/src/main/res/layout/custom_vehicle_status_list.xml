<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tvCustomVehicleList_Name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:fontFamily="@font/open_sans_semi_bold"
        android:maxLines="1"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvCustomVehicleList_Date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:fontFamily="@font/open_sans_semi_bold"
        android:maxLines="1"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/tvCustomVehicleList_Route"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:fontFamily="@font/open_sans_regular"
        android:maxLines="2" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tvCustomVehicleList_TrayStatus"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corner_blue"
                android:fontFamily="@font/open_sans_semi_bold"
                android:gravity="center"
                android:padding="7dp"
                android:text="  Tray Status  "
                android:textColor="@color/colorWhite" />

            <TextView
                android:id="@+id/tvCustomVehicleList_Type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:fontFamily="@font/open_sans_bold"
                android:gravity="end"
                android:padding="7dp"
                android:textAlignment="textEnd"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="16sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tvCustomVehicleList_ExtraTrayStatusOut"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corner_white"
                android:fontFamily="@font/open_sans_semi_bold"
                android:gravity="center"
                android:padding="7dp"
                android:text="Extra Tray Out"
                android:textColor="@color/colorPrimaryDark" />

            <TextView
                android:id="@+id/tvCustomVehicleList_ExtraTrayStatusIn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corner_white"
                android:fontFamily="@font/open_sans_semi_bold"
                android:gravity="center"
                android:padding="7dp"
                android:text="Extra tray In"
                android:textColor="@color/colorPrimaryDark" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tvCustomVehicleList_OutKm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corner_white"
                android:fontFamily="@font/open_sans_semi_bold"
                android:gravity="center"
                android:padding="7dp"
                android:text="Out Km - 0"
                android:textColor="@color/colorPrimaryDark" />

            <TextView
                android:id="@+id/tvCustomVehicleList_InKm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corner_white"
                android:fontFamily="@font/open_sans_semi_bold"
                android:gravity="center"
                android:padding="7dp"
                android:text="In Km - 0"
                android:textColor="@color/colorPrimaryDark" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tvCustomVehicleList_RunningKm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corner_white"
                android:fontFamily="@font/open_sans_semi_bold"
                android:gravity="center"
                android:padding="7dp"
                android:text="Running Km - 0"
                android:textColor="@color/colorPrimaryDark" />

            <TextView
                android:id="@+id/tvCustomVehicleList_diesel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corner_white"
                android:fontFamily="@font/open_sans_semi_bold"
                android:gravity="center"
                android:padding="7dp"
                android:text="Diesel - 0"
                android:textColor="@color/colorPrimaryDark" />

        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray" />


</LinearLayout>